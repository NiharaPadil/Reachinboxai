{
    "version": 2,
    "builds": [
      {
        "src": "react-app/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "react-app/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/react-app/build/$1"
      }
    ]
  }
  